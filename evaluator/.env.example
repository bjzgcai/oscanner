# Server Configuration
# Port for the FastAPI evaluator server (default: 8000)
PORT=8000

# Open Router API Key
# Get your API key from: https://openrouter.ai/keys
OPEN_ROUTER_KEY=sk-or-v1-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Gitee Access Tokens
# Generate tokens from: https://gitee.com/profile/personal_access_tokens

# Public Gitee (gitee.com) Access Token
PUBLIC_GITEE_TOKEN=your_public_gitee_token_here

# Enterprise Gitee (z.gitee.cn) Access Token
GITEE_TOKEN=your_enterprise_gitee_token_here
